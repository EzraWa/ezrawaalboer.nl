{% extends 'partials/base/base.twig' %}

{% block main %}

    {% setcontent homepage = "homepage" limit 1 returnsingle %}
    {% include 'partials/header/header.twig' with {
        'subTitle': homepage.subTitle,
        'title': homepage.title
    } %}

    {% setcontent experiences = 'experiences' where { status: 'published'} orderby 'rank' %}
    {% include 'partials/sections/experienceList.twig' with {
        'subTitle': 'Experiences',
        'title': 'Some lorum ipsum here',
        'experiences': experiences
    } %}

{% endblock main %}